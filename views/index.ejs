<% include partials/header %>
    <div id="mainCarousel"class="carousel slide" data-ride="carousel">
        <ol class="carousel-indicators">
            <li data-target="#mainCarousel" data-slide-to="0" class="active"></li>
            <li data-target="#mainCarousel" data-slide-to="1" ></li>
            <li data-target="#mainCarousel" data-slide-to="2" ></li>
        </ol>
        <div class="carousel-inner"  role="listbox">
            <div class="carousel-item active" style>
                <div class="carouselImage">
                    <img class="d-block w-100" src="/imgs/omarMasjid1.jpg" alt="First Slide" >
                </div>                
                <div class="carousel-caption d-flex h-100 align-items-center justify-content-center">
                    <div>
                        <h1 class=" display-1 font-weight-bold">Welcome to Omar Masjid</h1> 
                        <p class="display-3">Home For All</p>
                    </div>
                </div>
            </div>
            <div class="carousel-item ">
                <div class="carouselImage">
                    <img class="d-block w-100" src="/imgs/omarMasjid2.jpg" alt="Second Slide">
                </div>  
                <div class="carousel-caption d-flex h-100 align-items-center justify-content-center">
                    <div>
                        <h1 class="display-1 font-weight-bold">Second Slide</h1> 
                        <p class="display-3">Home For All</p>
                    </div>
                </div>
            </div>
            <div class="carousel-item">
                    <div class="carouselImage">
                        <img class="d-block w-100" src="/imgs/omarMasjid3.jpg" alt="Third Slide">
                    </div>    
                    <div class="carousel-caption d-flex h-100 align-items-center justify-content-center">
                        <div>
                            <h1 class="display-1 font-weight-bold">Third Slide</h1> 
                            <p class="display-3">Home For All</p>
                        </div>
                    </div>
            </div>
            <a class="carousel-control-prev" href="#mainCarousel" role="button" data-slide="prev">
                <span class="carousel-control-prev-icon"></span>
                <span class="sr-only">Previous</span>
            </a>
            <a class="carousel-control-next" href="#mainCarousel" role="button" data-slide="next">
                    <span class="carousel-control-next-icon"></span>
                    <span class="sr-only">Next</span>
            </a>
        </div>
    </div>

    <div id="servicesSection">
        <h1 class="display-1" style="text-align:center;">Services</h1>
        <div class="card-deck justify-content-around">
            <div class="row">
                <div class="col-sm-6 col-md-6 col-lg-4">
                    <div class="card">
                            <img src="/imgs/Quran1.jpg" class="card-img-top" alt="...">
                            <div class="card-body">
                                <h5 class="card-title">Quran Classes</h5>
                                <p class="card-text">Quran Classes.</p>
                                <a class="btn btn-primary">Learn More</a>
                            </div>
                    </div>
                </div>
                <div class="col-sm-6 col-md-6 col-lg-4">
                    <div class="card">
                        <img src="/imgs/masjid2.jpg" class="card-img-top" alt="...">
                        <div class="card-body">
                            <h5 class="card-title">Congregrational Prayers</h5>
                            <p class="card-text">Along with the Five Dailiy Prayers, Masjid Omar Also offers Taraweh Prayers, as well as Eid Prayers</p>
                            <a class="btn btn-primary">Prayer Timing</a>
                        </div>
                    </div>
                </div>
                <div class="col-sm-6 col-md-6 col-lg-4">
                    <div class="card">
                        <img src="/imgs/randomMasjid.jpg" class="card-img-top" alt="...">
                        <div class="card-body">
                            <h5 class="card-title">Girls and Boys Youth Group</h5>
                            <p class="card-text">Weekend Girls And Boys Youth Grousps</p>
                            <a class="btn btn-primary">Learn More</a>
                        </div>
                    </div>
                </div>              
            </div>
        </div>
    </div>

    <div id="aboutUsSection">
        <div id="aboutUsImage"></div>
        <h1 class="display-1" style="text-align:center;">About Us</h1>
        <div class="card-deck justify-content-around">
            <div class="row">
                <div class="col-sm-6 col-md-6 col-lg-4">
                    <div class="card">
                            <img src="/imgs/Quran1.jpg" class="card-img-top" alt="...">
                            <div class="card-body">
                                <h5 class="card-title">Our History</h5>
                                <p class="card-text">From our humble beginnings of a small masjid, to expanding to meet the needs of our growing diverse Community.</p>
                                <a class="btn btn-primary">Learn More About History</a>
                            </div>
                    </div>
                </div>
                <div class="col-sm-6 col-md-6 col-lg-4">
                    <div class="card">
                        <img src="/imgs/masjid2.jpg" class="card-img-top" alt="...">
                        <div class="card-body">
                            <h5 class="card-title">Board Members</h5>
                            <p class="card-text">Everyone from Imam, President, to those who take care of day to day needs of the masjid.</p>
                            <a class="btn btn-primary">Board Members</a>
                        </div>
                    </div>
                </div>
                <div class="col-sm-6 col-md-6 col-lg-4">
                    <div class="card">
                        <img src="/imgs/randomMasjid.jpg" class="card-img-top" alt="...">
                        <div class="card-body">
                            <h5 class="card-title">Islam</h5>
                            <p class="card-text">Learn more about Islam </p>
                            <a class="btn btn-primary">Learn More</a>
                        </div>
                    </div>  
                </div>
            </div>
        </div>
    </div>
    

